{
  "extends": "loopback",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "max-len": ["error", 100]
  }
}